// Internal
import {{camelName}}Actions from './actions';
{{#if util}}import util from './util';{{/if}}

// Constants
{{#if util}}const initialStore = util.buildInitialStore();{{/if}}
const { {{name}} } = {{camelName}}Actions;

export default function reducer(state = {{#if util}}initialStore{{else}}{}{{/if}}, { type, payload }) {
  switch (type) {
    case {{name}}.type: {
      return {
        ...state,
        payload,
      };
    }

    default:
      return state;
  }
}
